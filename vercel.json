{
  "builds": [
    { "src": "HomePage/dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/HomePage/dist/$1" }
  ],
  "installCommand": "cd HomePage && npm i --no-fund --no-audit",
  "buildCommand": "cd HomePage && npm run build",
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  }
}
